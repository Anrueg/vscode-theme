{
  "name": "zozzz-theme",
  "displayName": "Zozzz's Theme",
  "description": "Nice looking color theme",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zozzz/vscode-theme"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Zozzz - Brown",
        "uiTheme": "vs-dark",
        "path": "./themes/brown-theme.json"
      },
      {
        "label": "Zozzz - Cold",
        "uiTheme": "vs-dark",
        "path": "./themes/cold-theme.json"
      }
    ]
  },
  "scripts": {
    "start": "bun run --watch generate.ts",
    "build": "bun run generate.ts && vsce package"
  },
  "devDependencies": {
    "color": "^5.0.0"
  }
}
