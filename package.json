{
  "name": "anrueg",
  "displayName": "Anrueg",
  "description": "Nice looking color theme",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Anrueg/vscode-theme"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Anrueg - Brown",
        "uiTheme": "vs-dark",
        "path": "./themes/anrueg-brown-theme.json"
      },
      {
        "label": "Anrueg - Cold",
        "uiTheme": "vs-dark",
        "path": "./themes/anrueg-cold-theme.json"
      }
    ]
  },
  "scripts": {
    "start": "bun run --watch generate.ts",
    "build": "bun run generate.ts && vsce package"
  },
  "devDependencies": {
    "color": "^5.0.0"
  }
}
